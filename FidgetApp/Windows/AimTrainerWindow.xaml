<Window x:Class="FidgetApp.Windows.AimTrainerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FidgetApp.Windows"
        mc:Ignorable="d"
        Title="Aim Trainer" WindowState="Maximized" WindowStyle="None" ShowInTaskbar="False"
        AllowsTransparency="True" Background="Transparent" Topmost="True">
    <Grid>
        <Canvas x:Name="AppCanvas">
            <Ellipse Canvas.Left="500" Canvas.Top="500" Width="100" Height="100" Fill="AliceBlue" />
        </Canvas>
    </Grid>
</Window>
